{
    
    "manifest_version" : 2,
    "name" : "My Extension 2",
    "version": "1.0.0",
    "content_scripts":[
        {
        "matches": ["<all_urls>"],
        "js" : ["content.js"]
        }
    ],
    "browser_action":{
        "default_popup" :"popup.html",
        "default_title" :"Sticky Notes",
        "default_icon" :"icon.png"
    }

}